{
    "name": "thruster/http-modifiers",
    "type": "library",
    "description": "Thruster HttpModifier Collection of Modifiers",
    "keywords": ["http", "modifiers", "psr7", "psr-7", "request", "response", "thruster"],
    "homepage": "https://thruster.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Aurimas Niekis",
            "email": "aurimas@niekis.lt"
        }
    ],
    "require": {
        "php": ">=7.0",
        "thruster/http-modifier": "~2.0",
        "zendframework/zend-diactoros": "~1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": { "Thruster\\Component\\HttpModifiers\\": "src" }
